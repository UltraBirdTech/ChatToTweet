{
    "manifest_version": 2,
    "name": "Chat To Tweet",
    "short_name": "ChatToTweet",
    "version": "0.0.1",
    "description": "This is a expand tool for Goolgle Chrome. When typed to text box, same text posted to tweet too.",
    "icons": {
        "16": "ChatToTweet.png",
        "48": "ChatToTweet.png",
        "128": "ChatToTweet.png"
    },
    "content_scripts": [{
        "matches": ["http://127.0.0.1/*", "https://www.youtube.com/*"],
        "js": [
            "main.js"
        ]
    }],
    "browser_action": {
        "default_title": "DRMaker",
        "default_popup": "popup.html"
    }
}
